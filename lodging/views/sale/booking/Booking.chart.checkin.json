{
    "name": "Total Checkins",
    "description": "Checks how many checkins happen during the week we are in.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "layout": {
        "entity": "lodging\\sale\\booking\\Booking",
        "group_by": "range",
        "range_interval": "week",
        "range_from": "date.this.year.last",
        "range_to": "date.this.year.first",
        "datasets": [
            {
                "label": "CA rÃ©servations HTVA",
                "operation": ["COUNT", "object.id"],
                "domain":[[["date_from", ">=", "date.this.week.first"], ["date_from", "<=", "date.this.week.last"]]]
            }
        ]
    }
}