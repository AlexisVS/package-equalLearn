{
    "name": "Booking list",
    "description": "This view displays the list of bookings: the most recent on top.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "layout": {
        "entity": "lodging\\sale\\booking\\Consumption",
        "type": "bar",
        "stacked": false,
        "group_by": "range",
        "range_interval": "month",
        "range_from": "date.prev.year.first",
        "range_to": "date.prev.year.last",
        "datasets": [
            {
                "label": "tout public",
                "operation": ["COUNT", "object.id"],
                "domain"   : ["is_rental_unit", "=", true]
            }
        ]
    }
}