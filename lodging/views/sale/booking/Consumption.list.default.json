{
    "name": "Consumption",
    "description": "This view is intended for displaying the list of consumptions.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "group_by": ["date"],
    "limit": 100,
    "layout": {
        "items": [
            {
                "type": "field",
                "label": "Product",
                "value": "product_id",
                "width": "30%"
            },
            {
                "type": "field",
                "value": "qty",
                "width": "10%"
            },

            {
                "type": "field",
                "value": "date",
                "width": "0%",
                "visible": false
            },
            {
                "type": "field",
                "value": "schedule_from",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "schedule_to",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "is_rental_unit",
                "width": "10%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "rental_unit_id",
                "width": "15%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "is_meal",
                "width": "10%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "disclaimed",
                "width": "10%"
            }
        ]
    }
}