{
    "name": "Sale list",
    "description": "This view displays the list of Fundings",
    "access": {
        "groups": ["sale.default.user"]
    },
    "layout": {
        "entity": "sale\\pay\\Funding",
        "group_by": "range",
        "range_interval": "year",
        "range_from": "date.this.year.last",
        "range_to": "date.this.year.first",
        "datasets": [
            {
                "label": "Total remboursements",
                "operation": ["COUNT", "object.due_amount"],
                "domain": ["due_amount", "<", "object.paid_amount"]
            }
        ]
    }
}