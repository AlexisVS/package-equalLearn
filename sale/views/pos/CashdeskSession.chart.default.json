{
    "name": "Booking list",
    "description": "This view displays the list of bookings: the most recent on top.",
    "access": {
        "groups": ["pos.default.user"]
    },
    "layout": {
        "entity": "sale\\pos\\CashdeskSession",
        "group_by": "range",
        "range_interval": "year",
        "range_from": "date.this.year.first",
        "range_to": "date.this.year.last",
        "datasets": [
            {
                "label" : "CA Total POS",
                "operation": ["SUM", "object.amount"]
            }
        ]
    }
}