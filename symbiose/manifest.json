{
    "name": "symbiose",
    "description": "This package is used in order to group all packages forming Symbiose ERP.",
    "version": "2.0",
    "authors": ["Yesbabylon"],
    "license": "LGPL-3",
    "depends_on": [
        "core",
        "identity",
        "finance",
        "documents",
        "calendar",
        "stats",
        "support",
        "learn",
        "hr",
        "communication",
        "purchase",
        "realestate",
        "sale",
        "inventory",
        "timetrack",
        "talentlead"
    ],
    "requires": {
        "twig/intl-extra": "^3.9.2",
        "twig/twig": "^3.0"
    },
    "apps": [
        {
            "name": "Settings",
            "description": "Manage and configure global and App-specific options and parameters.",
            "extends": "settings",
            "icon": "settings",
            "color": "#FF5F00",
            "access": {
                "groups": [
                    "admins"
                ]
            },
            "params": {
                "menus": {
                    "left": "settings.left",
                    "top": "settings.top"
                }
            },
            "show_in_apps": true
        }
    ]
}
